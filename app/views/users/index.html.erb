<% @users.each do |user| %>
  <div class="panel panel-default member">
    <div class="panel-heading"><%= link_to user.number, user %></div>
    <div class="panel-body">
      <div class="thumbnailBox">
        <div class="noImage">No Image</div>
      </div>
      <div class="descriptionBox">
        <p><%= user.display_age %>ï¼š<%= truncate(user.display_name, length: 8) %></p>
        <span title="<%= user.description %>"><%= truncate(user.description, length: 30) %></span>
      </div>
    </div>
  </div>
<% end %>